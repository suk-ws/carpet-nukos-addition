{
	"schemaVersion": 1,
	"id": "carpet_nukos_addition",
	"version": "${version}",
	"name": "Carpet Nukos Addition",
	"description": "A carpet extension/addition provides features that used in Nukoland server.",
	"authors": [
		"A.C.Sukazyo Eyre"
	],
	"contact": {
		"homepage": "https://github.com/suk-ws/carpet-nukos-addition",
		"sources": "https://github.com/suk-ws/carpet-nukos-addition"
	},
	"license": "Apache 2.0",
	"icon": "assets/carpet_nukos_addition/icon.png",
